export-codecov-json
----
{
  "coverage": {}
}

set file1.go
----
file1.go

export-codecov-json
----
{
  "coverage": {
    "file1.go": {}
  }
}

set file1.go
1 0
2 10
3 1
----
file1.go
  1:0
  2:10
  3:1

export-codecov-json
----
{
  "coverage": {
    "file1.go": {
      "1": 0,
      "2": 10,
      "3": 1
    }
  }
}

set file2.go
----
file1.go
  1:0
  2:10
  3:1
file2.go

